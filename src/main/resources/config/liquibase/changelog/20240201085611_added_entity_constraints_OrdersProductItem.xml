<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity OrdersProductItem.
    -->
    <changeSet id="20240201085611-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="product_id"
                                 baseTableName="orders_product_item"
                                 constraintName="fk_orders_product_item__product_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product"/>

        <addForeignKeyConstraint baseColumnNames="orders_id"
                                 baseTableName="orders_product_item"
                                 constraintName="fk_orders_product_item__orders_id"
                                 referencedColumnNames="id"
                                 referencedTableName="orders"/>
    </changeSet>
</databaseChangeLog>
